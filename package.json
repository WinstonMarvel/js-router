{
  "name": "js-router",
  "version": "1.0.0",
  "description": "Simple declarative routing for HTML Components",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "postbuild": "uglifyjs lib/index.js --output lib/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "author": "Winston Jude",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WinstonMarvel/js-router.git"
  },
  "keywords": [
    "router",
    "javascript",
    "HTML5",
    "es6"
  ],
  "bugs": {
    "url": "https://github.com/WinstonMarvel/js-router/issues"
  },
  "homepage": "https://github.com/WinstonMarvel/js-router#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2",
    "uglify-js": "^3.6.9"
  },
  "files": [
    "lib/**/*"
  ]
